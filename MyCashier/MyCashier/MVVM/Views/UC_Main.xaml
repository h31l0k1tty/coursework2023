<UserControl x:Class="MyCashier.MVVM.Views.UC_Main"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:MyCashier.MVVM.Views"
             xmlns:viewmodels="clr-namespace:MyCashier.MVVM.ViewModels"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <UserControl.DataContext>
        <viewmodels:UC_MainVM/>
    </UserControl.DataContext>
    <Grid>
        <StackPanel>
            <Button Margin='5' Command="{Binding GoToAuthotisationCmd}">Назад</Button>
        </StackPanel>
    </Grid>
</UserControl>
